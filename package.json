{
  "name": "swifts-search-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "fastify": "^1.7.0",
    "fastify-static": "^0.12.0",
    "glob": "^7.1.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "search-tree-trie": "^0.3.0"
  },
  "devDependencies": {
